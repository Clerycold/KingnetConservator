<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/packrelative">

    <clery.kingnetconservator.app.kingnetconservator.Control.UIpattern.Titledesign
        android:id="@+id/packagetitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/shadow"/>

    <clery.kingnetconservator.app.kingnetconservator.Control.UIpattern.Backbutton
        android:id="@+id/backbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ListView
        android:id="@+id/packlist"
        android:layout_below="@+id/packagetitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:divider = "@android:color/white"
        />



</RelativeLayout>